{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ChatGPT LaTeX Copy Tool",
  "version": "1.1",
  "description": "Copies LaTeX expressions to clipboard and shows a tooltip on hover.",
  "permissions": ["clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png",
    "190": "190.png"
  }
}
